{
  "name": "treehaus",
  "version": "0.7.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/treeHouse.js",
    "dev": "ts-node-esm src/treeHouse.ts",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "chalk-animation": "^1.6.0",
    "cli-table3": "^0.6.2",
    "fast-glob": "^3.2.12"
  },
  "devDependencies": {
    "@types/chalk-animation": "^1.6.3",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
